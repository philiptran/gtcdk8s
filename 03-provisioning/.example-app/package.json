{
  "name": "gtcdk8s-03-provisioning-example-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:ui": "webpack --config ./webpack.config.js",
    "db:check": "node ./db/check.js",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "db:update": "npm run db:check && npm run db:migrate && npm run db:seed",
    "dev:api": "node ./index.js",
    "dev:ui": "cross-env NODE_ENV=development webpack-serve ./webpack.config.js",
    "predev": "docker-compose -f ./provisioning/compose/docker-compose.yml build",
    "dev": "docker-compose -f ./provisioning/compose/docker-compose.yml up dev-ui dev-api",
    "predev:db-update": "npm run predev",
    "dev:db-update": "docker-compose -f ./provisioning/compose/docker-compose.yml up dev-db-updater"
  },
  "keywords": [],
  "author": "zephinzer",
  "license": "MIT",
  "dependencies": {
    "@usvc/boilerplate": "^0.2.0",
    "convict": "^4.3.2",
    "knex": "^0.15.2",
    "mysql2": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/preset-react": "^7.0.0-rc.3",
    "@material-ui/core": "^1.5.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
