    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "koa-connect": "^2.0.1",
    "moment": "^2.22.2",
    "nodemon": "^1.18.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
